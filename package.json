{
    "name": "fbp",
    "displayName": "language-fbp",
    "description": "FBP flow definition language support for Visual Studio Code",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/LeuisKen/language-fbp"
    },
    "contributes": {
        "languages": [{
            "id": "fbp",
            "aliases": ["FBP flow definition langua", "fbp"],
            "extensions": [".fbp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "fbp",
            "scopeName": "source.fbp",
            "path": "./syntaxes/fbp.tmLanguage"
        }]
    }
}
